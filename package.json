{
  "name": "spider-backend",
  "version": "2.0.0",
  "description": "Facebook Ad Library Scraper API - Node.js",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "worker": "node src/worker.js",
    "dev:worker": "node --watch src/worker.js",
    "start:all": "node start.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\""
  },
  "keywords": [
    "facebook",
    "ads",
    "scraper",
    "apify",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apify-client": "^2.8.2",
    "axios": "^1.6.5",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "multer": "^2.0.2",
    "openai": "^4.20.1",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
